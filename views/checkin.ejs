<% layout('layout') -%>
    <!-- Main form 
    ================================================== -->
    <!-- Wrap the rest of the page in another container to center all the content. -->
    <div class="container marketing">
    <div class="row-fluid">
        <div class="span6 offset3" id="rbody">
          <h2 class="myh">Salt Reducer Checkins</h2>
	  <table class="table">
	    <thead>
	      <tr>
		<td>
		  <b> Checkin </b>
		</td>
		<td>
		  <b> Update </b>
		</td>  
	      </tr>
	    </thead>
	    <tbody>
	      <% for (var i = 0; i<c.length; i++) { 
					     if (c[i].data) { %>
	         <tr>
		   <td>
		     <a href='/checkin/<%=c[i].id%>'><%= c[i].data %></a>
		   </td>
		   <td>
		     <a class="btn btn-primary", href='/checkin/<%=c[i].id%>/edit'> Edit </a>
		     <a class="btn btn-danger", href='/checkin/<%=c[i].id%>'> Delete </a>
		   </td>
		 </tr>
		 <%   }
		      } %>  
	    </tbody>
	  </table>

        </div>
    </div>

    <!-- /END MAIN FORM  -->
</div>
<!-- /.container -->
