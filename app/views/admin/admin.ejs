<% layout('../layouts/default') -%>

<div class="container">
	<div class="row">
		<div class="col-md-8 col-md-offset-2">

			<div class="page-header">
				<h2>Administration</h2>
			</div>

			<% for(var i = 0; i < users.length; i++) { %>
				<div class="row stats">
					<div class="col-sm-4">
						<h3 class="text-muted"><%= users[i].name %></h3>
					</div>
				</div>
			<% } %>

		</div>
	</div>
</div>
